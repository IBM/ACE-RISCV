+#include <dt-bindings/clock/eic7700-clock.h>
+
+#define UART0_INT	100
+#define UART1_INT	101
+#define UART2_INT	102
+#define UART3_INT	103
+#define UART4_INT	104
+
+/ {
+	#address-cells = <2>;
+	#size-cells = <2>;
+	compatible = "eic7700-dev";
+
